{
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "strict": false,
    "jsx": "react-jsx",
    "types": [
      "react/experimental"
    ]
  },
  "include": [
    "src"
  ]
}